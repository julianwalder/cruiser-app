{
  "name": "@cruiser/frontend",
  "version": "1.0.0",
  "description": "React frontend for Cruiser Aviation Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:dev": "cd ../.. && wrangler deploy",
    "worker:dev": "cd ../.. && wrangler dev --env local",
    "worker:build": "cd ../.. && wrangler build",
    "worker:deploy": "cd ../.. && wrangler deploy",
    "dev:simple": "vite --port 3000",
    "api:dev": "node scripts/dev-api.js",
    "dev:setup": "node scripts/dev-setup.js",
    "setup:cloudflare-vars": "./scripts/setup-cloudflare-vars.sh",
    "dev:api:watch": "nodemon --watch scripts/dev-api.js scripts/dev-api.js",
    "dev:all": "concurrently \"npm run api:dev\" \"npm run dev:simple\"",
    "dev:complete": "./scripts/dev-all.sh",
    "dev:full": "./scripts/dev-all.sh",
    "dev:local": "concurrently \"npm run api:dev\" \"npm run dev:simple\""
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@react-google-maps/api": "^2.19.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@veriff/incontext-sdk": "^2.4.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "firebase": "^10.7.1",
    "html2canvas": "^1.4.1",
    "jose": "^6.0.11",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.2",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-firebase-hooks": "^5.1.1",
    "react-geocode": "^0.2.3",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-leaflet": "^4.2.1",
    "react-modal": "^3.16.1",
    "react-qr-code": "^2.0.12",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "react-virtualized": "^9.22.5",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/html2canvas": "^1.0.0",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.14.202",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.37",
    "@types/react-datepicker": "^4.19.4",
    "@types/react-dom": "^18.2.15",
    "@types/react-geocode": "^0.2.3",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^4.21.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^4.23.0"
  }
}
